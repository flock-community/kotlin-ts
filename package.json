{
  "name": "@flock/ts-plus-kotlin",
  "version": "0.0.0",
  "author": "Kasper Peulen",
  "license": "MIT",
  "repository": "https://github.com/flock-community/ts-plus-kotlin.git",
  "typesVersions": {
    "*": {
      "*": [
        "./build/dts/*"
      ]
    }
  },
  "exports": {
    "./*": {
      "require": "./build/cjs/*.js",
      "import": "./build/esm/*.js"
    }
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:esm && npm run build:cjs",
    "build:cjs": "cp .swcrc dist && cd dist && swc ./esm -d ./cjs",
    "build:clean": "rimraf dist",
    "build:esm": "tsc -b tsconfig.json",
    "changeset": "changeset",
    "postinstall": "tsplus-install",
    "release": "changeset publish",
    "swc": "swc",
    "test": "jest",
    "update": "npx --yes update-ruecksichtslos@latest --caret"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.3",
    "@changesets/cli": "^2.21.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.150",
    "@tsplus/installer": "^0.0.34",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "prettier": {
    "printWidth": 120
  }
}
