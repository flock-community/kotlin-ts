{
  "name": "@flock/kotlin-ts",
  "version": "0.0.7",
  "author": "Kasper Peulen",
  "license": "MIT",
  "repository": "https://github.com/flock-community/kotlin-ts.git",
  "main": "./index.js",
  "module": "./esm/index.js",
  "sideEffects": [],
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./index.js"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./*.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "release": "npm t && npm run build && cd build && npm publish",
    "build": "npm run build:clean && npm run build:esm && npm run build:cjs && cp package.json build/package.json && cp package.esm.json build/esm/package.json",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:esm": "tsc -b tsconfig.esm.json",
    "build:clean": "rimraf build",
    "postinstall": "tsplus-install",
    "test": "jest --all",
    "update": "npx --yes update-ruecksichtslos@latest --caret"
  },
  "devDependencies": {
    "@tsplus/installer": "^0.0.41",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "prettier": {
    "printWidth": 120
  }
}
