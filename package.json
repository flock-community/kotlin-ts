{
  "name": "@flock/ts-plus-kotlin",
  "version": "0.0.0",
  "author": "Kasper Peulen",
  "license": "MIT",
  "repository": "https://github.com/flock-community/ts-plus-kotlin.git",
  "typesVersions": {
    "*": {
      "*": [
        "./build/dts/*"
      ]
    }
  },
  "exports": {
    "./*": {
      "require": "./build/cjs/*.js",
      "import": "./build/esm/*.js"
    }
  },
  "scripts": {
    "test": "jest",
    "changeset": "changeset",
    "release": "changeset publish",
    "build": "yarn build:clean && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc -b tsconfig.json",
    "build:cjs": "cp .swcrc dist && cd dist && swc ./esm -d ./cjs",
    "build:clean": "rimraf dist",
    "postinstall": "tsplus-install",
    "swc": "swc"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.2",
    "@changesets/cli": "^2.20.0",
    "@swc/cli": " ^0.1.55",
    "@swc/core": "^1.2.137",
    "@tsplus/installer": "^0.0.29",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.13",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "ts-jest": "^27.1.3"
  },
  "prettier": {
    "printWidth": 120
  }
}
